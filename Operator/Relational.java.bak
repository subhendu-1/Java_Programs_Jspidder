
//Relational Operator:- It is used to check the realtion between tow oparend and it will tige the result in boolean value.
class Relational 
{
	public static void main(String[] args) 
	{
		int a = 5;
		
		 int b =  a++;   // first value is store in same 'a' memory location after increment this value
		 System.out.println(a);

		 //int b  = a++ + a;
		System.out.println(b);  // here continue the execution that's why here value is incremented that's why after '+' we get the value 'a' as a 6
		System.out.println(a < 6);
		System.out.println(a++ > 6);
		System.out.println(++a >= 6);  // ture becuse
		System.out.println(a < 6);
		System.out.println(10 < 20);
		System.out.println(10 > 13);
		System.out.println(10 >= 11);
		System.out.println(10 == 10);
		System.out.println(10 == 20);
		System.out.println(10 != 20);
		System.out.println(10 <= 10);
		System.out.println(10 != 20);
	}
}
