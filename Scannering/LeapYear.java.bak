import java.util.*;
class LeapYear 
{
	public static void main(String[] args) 
	{
		Scanner a = new Scanner(System.in);


		System.out.println("Enter the year ");

		int b = a.nextInt();

		if((a%400) == 0){
			System.out.println("This is leap year");
		}

		else if((a%4) == 0 && (a%100) != 0){
			System.out.println("This is leap year");
		}

		else{
			System.out.println ("This is not a leap year");
		}


	}
}
